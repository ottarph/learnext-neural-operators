{
    "comment": "Permutation invariant DeepSets-DeepONet that reaches 0.11 minimum determinant of deformation gradient on learnext p1 dataset, trained on artificial dataset.",
    "branch": {
        "DeepSets": {
            "representer": {
                "MLP": {"widths": [4, 256, 256, 256, 256], "activation": "ReLU"}
            },
            "processor": {
                "MLP": {"widths": [256, 256, 32], "activation": "ReLU"}
            },
            "reduction": "mean"
        }
    },
    "trunk": {
        "MLP": {
            "widths": [2, 256, 256, 256, 256, 256, 32],
            "activation": "ReLU"
        }
    },
    "branch_encoder": {
        "SequentialEncoder": [
            {"CoordinateInsertEncoder": {}},
            {"InnerBoundaryFilterEncoder": {}},
            {"RandomPermuteEncoder": {"dim": -2, "unit_shape_length": 2}}
        ]
    },
    "trunk_encoder": {
        "IdentityEncoder": {}
    },
    "combine_style": 2,
    "final_bias": false,
    "mask_function_f": "2.0 * (x[0]+1.0) * (1-x[0]) * exp( -3.5*pow(x[0], 7) ) + 0.1",
    "dataset": {
        "directory": "dataset/artificial_learnext",
        "style": "folders",
        "batch_size": 256,
        "train_val_split": [0.7, 0.3]
    },
    "loss_fn": {
        "RelativeMSELoss": {
            "eps": 0.001
        }
    },
    "optimizer": {
        "Adam": { "lr": 0.001
        }
    },
    "scheduler": {
        "ReduceLROnPlateau": {
            "factor": 0.5
        }
    },
    "num_epochs": 6000,
    "seed": 0
}
