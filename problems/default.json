{
    "branch": {
        "MLP": {
            "widths": [412, 256, 256, 256, 256, 256, 256, 256, 32],
            "activation": "ReLU"
        }
    },
    "trunk": {
        "MLP": {
            "widths": [2, 256, 256, 256, 256, 256, 256, 256, 32],
            "activation": "ReLU"
        }
    },
    "branch_encoder": {
        "SequentialEncoder": [
            {"InnerBoundaryFilterEncoder": {}},
            {"FlattenEncoder": {"start_dim": -2}}
        ]
    },
    "trunk_encoder": {
        "IdentityEncoder": {}
    },
    "combine_style": 2,
    "final_bias": false,
    "mask_function_f": "2.0 * (x[0]+1.0) * (1-x[0]) * exp( -3.5*pow(x[0], 7) ) + 0.1",
    "dataset": {
        "directory": "dataset/artificial_learnext",
        "style": "folders",
        "batch_size": 256,
        "train_val_split": [0.7, 0.3]
    },
    "loss_fn": {
        "RelativeMSELoss": {
            "eps": 0.001
        }
    },
    "optimizer": {
        "Adam": {
        }
    },
    "scheduler": {
        "ReduceLROnPlateau": {
            "factor": 0.5
        }
    },
    "num_epochs": 4000,
    "seed": 0
}
