{
    "model": {
        "DeepONet": {
            "input_domain_dim": 2,
            "input_range_dim": 2,
            "output_domain_dim": 2,
            "output_range_dim": 2, 
            "branch": {
                "MLP": {
                    "widths": [1760, 256, -1],
                    "activation": "Tanh"
