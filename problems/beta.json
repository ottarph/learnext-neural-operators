{
    "branch": {
        "MLP": {
            "widths": [880, 256, 256, 256, 256, 256, 256, 256, 32],
            "activation": "ReLU"
        }
    },
    "trunk": {
        "MLP": {
            "widths": [2, 256, 256, 256, 256, 256, 256, 256, 32],
            "activation": "ReLU"
        }
    },
    "width": 16,
    "final_bias": false,
    "mask_function_f": "2.0 * (x[0]+1.0) * (1-x[0]) * exp( -3.5*pow(x[0], 7) ) + 0.1",
    "dataset": {
        "directory": "dataset/artificial_learnext",
        "style": "folders",
        "batch_size": 256
    },
    "loss_fn": {
        "L1Loss": {
        }
    },
    "optimizer": {
        "Adam": {
        }
    },
    "scheduler": {
        "ReduceLROnPlateau": {
            "factor": 0.5
        }
    },
    "num_epochs": 400
}
